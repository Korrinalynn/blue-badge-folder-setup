 {\r\n    return (\r\n        <footer>\r\n            <row>\r\n                <p>&copy; Eleven Fifty 2020</p>\r\n            </row>\r\n        </footer>\r\n    );\r\n};\r\n\r\nexport default Footer;","C:\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\Components\\Site\\Sidebar.js",[],"C:\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\Components\\Site\\Header.js",[],"C:\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\Components\\Site\\Home.js",[],"C:\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\Components\\Site\\Resources.js",[],"C:\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\Components\\Concepts\\FunctionalComponentDemo.js",[],"C:\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\Components\\Concepts\\JSXRules.js",["92"],"import React from 'react';\r\n\r\nconst JSXRules = () => {\r\n    return (\r\n        <div className=\"main\">\r\n            <div className=\"mainDiv\">\r\n                <h1 className=\"section-title\">JSX</h1>\r\n                <dl>\r\n                    <dt>Resembles HTML</dt>\r\n                    <dd>It's not. It's actually closer to JavaScript.</dd>\r\n                    <dt>React Elements</dt>\r\n                    <dd>These are used to construct and update the DOM, or what you see on the screen.</dd>\r\n                    <dt>Not Required</dt>\r\n                    <dd>You can write in vanilla JS, but most sane people use JSX</dd>\r\n                </dl>\r\n                <hr />\r\n                <NormalComponent />\r\n                <CreateElementComponent />\r\n            </div>\r\n        </div>\r\n    );\r\n};\r\n\r\nexport default JSXRules;\r\n\r\nconst NormalComponent = () => {\r\n    return (\r\n        <div style={{ border: '2px solid black' }}>\r\n            <h1>Normal Functional Component</h1>\r\n            <p>This was constructed with JSX in the return.</p>\r\n            <img src=\"https://pbs.twimg.com/media/DOzL82mXkAA0zFs.jpg\" />\r\n        </div>\r\n    );\r\n};\r\n\r\nconst CreateElementComponent = () => {\r\n    return (\r\n        React.createElement('div', { style: { border: '2px solid black' } },\r\n            React.createElement('h1', null, 'CreateElementComponent'),\r\n            React.createElement('p', null, 'This was constructed with CreateElement calls in the return'),\r\n            React.createElement('img', { src: 'https://cdn-images-1.medium.com/max/1200/1*jJZHFQmhkq_7ohn18KrMhA.png' }, null)\r\n        )\r\n    );\r\n};","C:\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\Components\\Concepts\\State.js",["93"],"import React, {useState} from 'react';\r\n\r\nconst State = () => {\r\n    return (\r\n        <div className=\"main\">\r\n            <div className=\"mainDiv\">\r\n                <ul>\r\n                    <dt>useState is Unique to Functional Components</dt>\r\n                    <dd>Class components have a seperate means of using and changing state.</dd>\r\n                    <dt>useState uses Array Destructuring</dt>\r\n                    <dd>useState provides a state variable and a setState function</dd>\r\n                    <dt>useState is a Hook</dt>\r\n                    <dd>useState is a Hook baked into React</dd>\r\n                    <dt>Triggers Re-renders</dt>\r\n                    <dd>Like with props changes, changing the state of a component re-renders the whole component.</dd>\r\n                </ul>\r\n                <StateExample />\r\n            </div>\r\n        </div>\r\n    );\r\n};\r\n\r\nexport default State;\r\n\r\nfunction StateExample() {\r\n    const [text, setText] = useState('');\r\n    const [likeNum, setLikeNum] = useState(0);\r\n    const [textColor, setTextColor] = useState('blue');\r\n\r\n    return(\r\n        <div>\r\n            <input value={text} onChange={e => setText(e.target.value)} />\r\n            <br />\r\n            <img style={{ width: '100px', height: '100px' }} src=\"https://upload.wikimedia.org/wikipedia/commons/1/13/Facebook_like_thumb.png\" onClick={e => setLikeNum(likeNum + 1)} />\r\n            <span>{likeNum}</span>\r\n            <br />\r\n            <br />\r\n            <p style={{ color: textColor }} onMouseEnter={e => setTextColor('red')} onMouseLeave={e => setTextColor('blue')}>\r\n            This should be able to change colors\r\n            </p>\r\n        </div>\r\n    );\r\n};","C:\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\Components\\Concepts\\Effects.js",[],"C:\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\Components\\Concepts\\PropsDemo.js",["94","95","96"],"import React, { useState } from 'react';\r\nimport PropTypes from 'prop-types';\r\n\r\nconst PropsDemo = () => {\r\n\r\n    const [ color, setColor ] = useState('white');\r\n    const [ backgroundColor, setBackgroundColor ] = useState('purple');\r\n    const [ borderRadius, setBorderRadius ] = useState('5px');\r\n    const [ borderStyle, setBorderStyle ] = useState('dashed');\r\n    const [ display, setDisplay ] = useState('inline-block');\r\n    const [ width, setWidth ] = useState('350px');\r\n    const [ textAlign, setTextAlign ] = useState('center');\r\n\r\n    let styles = {\r\n        color : color,\r\n        backgroundColor : backgroundColor,\r\n        borderRadius: borderRadius,\r\n        borderStyle : borderStyle,\r\n        display : display,\r\n        width : width,\r\n        textAlign: textAlign,\r\n    };\r\n\r\n    const toggleColor = () => {\r\n        color === 'white' ? setColor('pink') : setColor('white');\r\n    }\r\n    const toggleBackgroundColor = () => {\r\n        backgroundColor === 'purple' ? setBackgroundColor('black') : setBackgroundColor('purple');\r\n    }\r\n    const toggleBorderRadius = () => {\r\n        borderRadius === '5px' ? setBorderRadius('20px') : setBorderRadius('5px');\r\n    }\r\n    const toggleBorderStyle = () => {\r\n        borderStyle === 'dashed' ? setBorderStyle('double') : setBorderStyle('dashed');\r\n    }\r\n\r\n    return (\r\n        <div className=\"main\">\r\n            <div className=\"mainDiv\">\r\n                <div style={styles}>\r\n                <FunctionalComponent string=\"Will this display?\" function={toggleColor} selectedStyle={ color } />\r\n                <FunctionalComponent string=\"Props are pretty cool, right?\" function={toggleBackgroundColor} selectedStyle={ backgroundColor } />\r\n                <FunctionalComponent string=\"You can send data from one component ...\" function={toggleBorderRadius} selectedStyle={ borderRadius } />\r\n                <FunctionalComponent string=\"... to another!\" function={toggleBorderStyle} selectedStyle={ borderStyle }/>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    );\r\n}\r\n\r\nexport default PropsDemo;\r\n\r\nconst FunctionalComponent = (props) => {\r\n    return (\r\n        <div>\r\n            <p>{ props.string }</p>\r\n            <button onClick={ props.function }>Toggle Style</button>\r\n            <TinyComponent selectedStyle={ props.styleSelected } />\r\n        </div>\r\n    )\r\n}\r\n\r\nconst TinyComponent = (props) => {\r\n    return (\r\n        <div>\r\n            <p>The current style is : { props.selectedStyle }</p>\r\n        </div>\r\n    )\r\n};\r\n\r\nFunctionalComponent.defaultProps = {\r\n    string: 'This is wild!',\r\n    function: () => console.log('Can I see this in my dev tools?'),\r\n    selectedStyle : 'what style?'\r\n};\r\n\r\nFunctionalComponent.propTypes = {\r\n    string: PropTypes.string.isRequired,\r\n    function: PropTypes.func.isRequired,\r\n    selectedStyle : PropTypes.string.isRequired\r\n};","C:\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\Components\\Concepts\\Hooks.js",["97"],"import React, {useState, useEffect} from 'react';\r\n\r\nconst Hooks = () => {\r\n    const [query, setQuery] = useState('');\r\n    const [results, setResults] = useState({});\r\n\r\n    const fetcher = () => {\r\n        fetch(`https://swapi.dev/api/people/${query}`)\r\n        .then(res => res.json())\r\n        .then(json => {\r\n            console.log(json)\r\n            setResults(json);\r\n        })\r\n    }\r\n    return(\r\n        <div className=\"main\">\r\n            <div className=\"mainDiv\">\r\n                <input value={query} onChange={e => setQuery(e.target.value)} placeholder=\"enter your sw character number\"/>\r\n                <button onClick={() => fetcher()}>Click for Character!</button>\r\n                {results ? <h2>{results.name}</h2> : <div></div>}\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nconst Hooks2 = () => {\r\n    const [results, queryNum, setQueryNum] = useNumHook('');\r\n    const [clicks, setClicks] = useClicks(0);\r\n\r\n    return(\r\n        <div className=\"main\">\r\n            <div className=\"mainDiv\">\r\n                <h3>Enter a number below to see a number fact</h3>\r\n                <input value={queryNum} onChange={e => setQueryNum(e.target.value)} placeholder=\"enter a number\"/>\r\n                {results ? <h2>{results}</h2> : <div></div>}\r\n                <button onClick={() => setClicks(clicks + 1)}>Click to update document title</button>\r\n            </div>\r\n        </div>\r\n    );\r\n};\r\n\r\nconst useNumHook = (num) => {\r\n    const [queryNum, setQueryNum] = useState(num);\r\n    const [results, setResults] = useState('');\r\n\r\n    useEffect(() => {\r\n        if (queryNum !== ''){\r\n            fetch(`http://numbersapi.com/${queryNum}`)\r\n            .then(res => res.text())\r\n            .then(json => {\r\n                setResults(json);\r\n                console.log(json);\r\n            });\r\n        };\r\n    }, [queryNum])\r\n\r\n    return [results, queryNum, setQueryNum];\r\n}\r\n\r\nconst useClicks = (initCount) => {\r\n    const [clicks, setClicks] = useState(initCount);\r\n\r\n    useEffect(() => {\r\n        document.title = `You have clicked ${clicks} times`;\r\n    }, [clicks])\r\n    return [clicks, setClicks]\r\n}\r\n\r\nexport default Hooks2;","C:\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\Components\\Apps\\Timer-Apps\\TimePiecesApp.js",[],"C:\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\Components\\Apps\\Timer-Apps\\TimerApp.js",[],"C:\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\Components\\Apps\\Timer-Apps\\ClockApp.js",["98"],"import React, { useState, useEffect } from 'react';\r\n\r\nconst getTimeString = () => {\r\n    const date = new Date(Date.now()).toLocaleTimeString();\r\n    return date;\r\n}\r\n\r\nconst ClockApp = () => {\r\n    const [time, setTime] = useState(getTimeString());\r\n\r\n    useEffect(() => {\r\n        const interval = setInterval(() => {\r\n            let date = getTimeString();\r\n            setTime(date);\r\n        }, 1000);\r\n        return () => clearInterval(interval);\r\n    }, []);\r\n    \r\n    return (\r\n        <div>\r\n            <h1 className=\"section-title\">React Clock</h1>\r\n            <h1 className=\"explanation\" />\r\n            <p>{time}</p>\r\n        </div>\r\n    );\r\n}\r\n\r\nexport default ClockApp;","C:\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\Components\\Apps\\Timer-Apps\\StopWatchApp.js",[],"C:\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\Components\\Apps\\nyt-app\\NytApp.js",[],"C:\\ElevenFiftyProjects\\BlueBa